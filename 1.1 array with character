DATE - 11/06/2025 


#include <iostream>
using namespace std;

void printarray(char arr[], int size) {
    cout << "Printing the array: " << endl;

    for (int i = 0; i < size; i++) {
        cout << arr[i] << " ";
    }
    cout << "\nPRINTING DONE" << endl;
}

int main() {
    char first[15] = {'A', 'B', 'C'};

    int n = 15;
    printarray(first, n);

    char second[20] = {'H', 'e', 'l', 'l', 'o'};

    int m = 20;
    printarray(second, m);

    char third[5] = {'Z'};

    int q = 5;
    printarray(third, q);

    return 0;
}


