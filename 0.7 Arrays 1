#include <iostream>
using namespace std;


int main() {
    int numbers[5];  
    int sum = 0;

    cout << "Enter 5 numbers: " << endl;

    
    for (int i = 0; i < 5; i++) {
        cout << "Number " << i + 1 << ": ";
        cin >> numbers[i];
        sum += numbers[i]; 
    }

    
    cout << "\nThe sum of the numbers is: " << sum << endl;

    return 0;
}
